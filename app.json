{
  "expo": {
    "name": "DevShop",
    "slug": "DevShop",
    "version": "1.0.0",
    "scheme": "devshop",
    "orientation": "portrait",
    "icon": "./src/assets/images/hero.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/billboard-bg-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devshop"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/hero.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devshop"
    },
    "web": {
      "favicon": "./src/assets/images/hero.jpeg"
    }
  }
}
